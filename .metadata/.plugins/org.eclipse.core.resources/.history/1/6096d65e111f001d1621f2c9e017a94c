package Basic;



public class Math {
    public void add(int a, int b)
    {
        System.out.println("Summ is :" + a+b);
    }
    public void add(int a, int b, int c)
    {
        System.out.println("Summ is :" + a+b+c);
    }
    public void multiply(int a, int b)
    {
        System.out.println("Product is :" + a*b);
    }



}